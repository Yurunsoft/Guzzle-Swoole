{
    "require": {
        "guzzlehttp/guzzle": "{guzzleVersion}",
        "yurunsoft/guzzle-swoole": "@dev"
    },
    "require-dev": {
        "phpunit/phpunit": ">=7"
    },
    "autoload-dev": {
        "psr-4": {
            "Yurun\\Util\\Swoole\\Guzzle\\Test\\": "{guzzleSwoolePath}/test/"
        }
    },
    "repositories": [
        {
            "type": "path",
            "url": "{guzzleSwoolePath}"
        }
    ]
}